{
    "name": "bahamut-automation",
    "version": "1.0.0-pre.2",
    "description": "免費開源的巴哈姆特自動化！",
    "main": "src/core/index.js",
    "bin": {
        "ba": "./src/cli.js",
        "baha": "./src/cli.js"
    },
    "scripts": {
        "prepare": "npm audit fix --silent",
        "start": "node src/cli.js",
        "build": "pkg .",
        "format": "prettier --write \"src/**/*.js\""
    },
    "keywords": [
        "bahamut",
        "automation"
    ],
    "author": {
        "name": "JacobLinCool",
        "email": "jacoblincool@gmail.com"
    },
    "license": "MIT",
    "dependencies": {
        "@actions/core": "^1.6.0",
        "@actions/github": "^5.0.0",
        "@octokit/core": "^3.5.1",
        "@sentry/node": "^6.15.0",
        "@sentry/tracing": "^6.15.0",
        "countapi-js": "^1.0.2",
        "html-to-text": "^8.1.0",
        "markdown-it": "^12.2.0",
        "node-fetch": "^2.6.6",
        "otplib": "^12.0.1",
        "playwright": "^1.16.3",
        "turndown": "^7.1.1"
    },
    "devDependencies": {
        "pkg": "^5.4.1",
        "prettier": "^2.4.1"
    },
    "pkg": {
        "scripts": [
            "src/**/*.js"
        ],
        "assets": [],
        "targets": [
            "node16-macos-x64",
            "node16-linux-x64",
            "node16-win-x64"
        ],
        "outputPath": "dist"
    },
    "homepage": "https://github.com/JacobLinCool/bahamut-automation#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JacobLinCool/bahamut-automation.git"
    },
    "bugs": {
        "url": "https://github.com/JacobLinCool/bahamut-automation/issues"
    }
}
